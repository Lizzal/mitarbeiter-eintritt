      <bpmn:outgoing>SequenceFlow_05q80su</bpmn:outgoing>
      <bpmn:script>new File('Log.txt').write("Name : " + first_name + " " + last_name + " --&gt; Created Username in AD: " + username);</bpmn:script>
    </bpmn:scriptTask>
